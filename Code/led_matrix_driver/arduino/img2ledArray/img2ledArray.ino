    for (int j = 0; j < (matrix.width() - 2) * aspectRatio; j++) {
      index = 3 * (i * matrix.height() + j);
      r = pgm_read_byte(img + index);
      g = pgm_read_byte(img + index + 1);
      b = pgm_read_byte(img + index + 2);
      c = matrix.Color888(r, g, b);
